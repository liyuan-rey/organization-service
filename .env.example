# Environment Variables Template
# For local development, copy this file to .env.local and customize
# For other environments, set these variables in your deployment platform
# Never commit actual credentials to version control

# Database Configuration
POSTGRES_DB=your_database_name
POSTGRES_USER=your_database_user
POSTGRES_PASSWORD=your_database_password
DB_HOST=localhost
DB_PORT=5432

# Application Configuration
SPRING_PROFILES_ACTIVE=local
SERVER_PORT=8080

# JPA Configuration
SPRING_JPA_SHOW_SQL=false

# Logging Configuration
LOGGING_LEVEL_ROOT=INFO
LOGGING_LEVEL_COM_REYTHECODER_ORGANIZATION=INFO
LOGGING_LEVEL_SPRING_WEB=INFO
LOGGING_LEVEL_HIBERNATE_SQL=INFO
LOGGING_LEVEL_HIBERNATE_BINDER=INFO